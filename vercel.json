{
  "version": 2,
  "builds": [
    {
      "src": "introToDjango/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "introToDjango/wsgi.py"
    }
  ],
  "env": {
    "VERCEL": "1",
    "PYTHON_RUNTIME_VERSION": "3.9"
  }
}
